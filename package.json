{
  "name": "clock",
  "homepage": "https://rafaeltmbr.github.io/clock",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "lint": "eslint ./src/script --fix",
    "test": "jest",
    "dev:label-input:pug": "pug ./src/components/label-input/index.pug -o ./src/components/label-input/build --watch --pretty",
    "dev:label-input:sass": "sass ./src/components/label-input/label-input.sass ./src/components/label-input/build/style.css --watch --no-source-map",
    "dev:label-input:webpack": "webpack --mode=development --config ./src/components/label-input/webpack.config.js -w",
    "dev:label-input": "concurrently \"yarn dev:label-input:pug\" \"yarn dev:label-input:sass\" \"yarn dev:label-input:webpack\"",
    "webpack": "webpack --mode=production",
    "webpack-w": "webpack --mode=development -w",
    "pug": "pug ./src/markup/index -o ./public",
    "pug-w": "pug ./src/markup/index -o ./public --pretty --watch",
    "sass": "sass ./src/style/index.sass ./public/index.css --no-source-map",
    "sass-w": "sass ./src/style/index.sass ./public/index.css --no-source-map --watch",
    "build": "yarn pug & yarn sass & yarn webpack",
    "dev": "concurrently \"yarn pug-w\" \"yarn sass-w\" \"yarn webpack-w\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d public"
  }
}
