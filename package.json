{
  "name": "Clock",
  "homepage": "https://rafaeltmbr.github.io/clock",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "lint": "eslint ./src/script --fix",
    "translate": "babel ./main.js ./util.js -o ./bundle.js",
    "dev": "webpack --mode=development -w",
    "build": "webpack --mode=production",
    "execute": "yarn lint & yarn build & node ./public/bundle.js",
    "test": "jest",
    "pug": "pug ./src/markup/index/ -o ./public -P -w",
    "pug-w": "pug ./src/markup/index -o ./public --pretty --watch",
    "style": "sass ./src/style/index.sass ./public/index.css --no-source-map",
    "style-w": "sass ./src/style/index.sass ./public/index.css --no-source-map --watch",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d public"
  }
}
